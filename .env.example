# CodeForge Environment Variables
# Copy this file to .env and fill in your values

# AI Provider API Keys (optional - app works in mock mode without these)

# Get your Anthropic API key at: https://console.anthropic.com/
VITE_ANTHROPIC_API_KEY=

# Get your OpenAI API key at: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=

# Get your Google Gemini API key at: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=

# Get your OpenRouter API key at: https://openrouter.ai/keys
VITE_OPENROUTER_API_KEY=

# Default AI Provider: 'anthropic', 'openai', 'gemini', or 'openrouter'
VITE_DEFAULT_AI_PROVIDER=anthropic
